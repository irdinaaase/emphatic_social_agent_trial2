    "highContrast": {
      "primary": "#0066cc",
      "secondary": "#004080",
      "accent": "#0099ff",
      "background": "#000000",
      "surface": "#111111",
      "text": "#ffffff",
      "textSecondary": "#cccccc",
      "border": "#333333",
      "success": "#00ff00",
      "warning": "#ffff00",
      "danger": "#ff0000",
      "info": "#00ffff"
    },
    "sepia": {
      "primary": "#8b4513",
